{
    "name": "@eyra/feldspar",
    "version": "0.1.0",
    "main": "dist/index.js",
    "module": "dist/index.esm.js",
    "types": "dist/index.d.ts",
    "type": "module",
    "files": [
        "dist",
        "src/processing/py_worker.js",
        "dist/styles.css"
    ],
    "scripts": {
        "build:css": "NODE_ENV=production postcss ./src/framework/styles.css -o ./dist/styles.css",
        "build:ts": "rollup -c",
        "build": "pnpm run build:css && pnpm run build:ts",
        "dev": "concurrently \"rollup -c -w\" \"tailwindcss build -i ./src/framework/styles.css -o ./dist/styles.css --watch\"",
        "test": "jest",
        "clean": "rm -rf dist node_modules"
    },
    "peerDependencies": {
        "react": "19.2.4",
        "react-dom": "19.2.4",
        "tslib": "2.8.1",
        "typescript": "5.9.3"
    },
    "dependencies": {
        "@types/lodash": "4.17.23",
        "lodash": "4.17.23"
    },
    "exports": {
        ".": {
            "import": "./dist/index.esm.js",
            "types": "./dist/index.d.ts"
        },
        "./dist/styles.css": {
            "import": "./dist/styles.css"
        }
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "29.0.0",
        "@rollup/plugin-json": "6.1.0",
        "@rollup/plugin-node-resolve": "16.0.3",
        "@rollup/plugin-terser": "0.4.4",
        "@rollup/plugin-typescript": "12.3.0",
        "@rollup/plugin-url": "8.0.2",
        "@types/react": "19.2.14",
        "@types/react-dom": "19.2.3",
        "fdir": "6.5.0",
        "postcss": "^8.4.35",
        "postcss-cli": "^11.0.0",
        "postcss-import": "^16.0.1",
        "autoprefixer": "^10.4.18",
        "tailwindcss": "^3.4.1",
        "rollup": "4.58.0",
        "rollup-plugin-copy": "3.5.0",
        "rollup-plugin-dts": "6.3.0",
        "rollup-plugin-peer-deps-external": "2.2.4",
        "tslib": "2.8.1",
        "typescript": "5.9.3"
    }
}