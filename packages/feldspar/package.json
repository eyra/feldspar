{
    "name": "@eyra/feldspar",
    "version": "0.1.0",
    "main": "dist/index.js",
    "module": "dist/index.esm.js",
    "types": "dist/index.d.ts",
    "type": "module",
    "files": [
        "dist",
        "src/processing/py_worker.js",
        "dist/styles.css"
    ],
    "scripts": {
        "build:css": "NODE_ENV=production tailwindcss build -i ./src/framework/styles.css -o ./dist/styles.css --minify",
        "build:ts": "rollup -c",
        "build": "npm run build:css && npm run build:ts",
        "dev": "concurrently \"rollup -c -w\" \"tailwindcss build -i ./src/framework/styles.css -o ./dist/styles.css --watch\"",
        "test": "jest"
    },
    "peerDependencies": {
        "react": "^19.0.0",
        "react-dom": "^19.0.0"
    },
    "dependencies": {
        "@types/lodash": "^4.17.14",
        "lodash": "^4.17.21"
    },
    "exports": {
        ".": {
            "import": "./dist/index.esm.js",
            "types": "./dist/index.d.ts"
        },
        "./dist/styles.css": {
            "import": "./dist/styles.css"
        }
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "^25.0.7",
        "@rollup/plugin-json": "^6.1.0",
        "@rollup/plugin-node-resolve": "^15.2.3",
        "@rollup/plugin-typescript": "^11.1.6",
        "@rollup/plugin-terser": "^0.4.4",
        "@rollup/plugin-url": "^8.0.2",
        "@types/react": "^19.0.7",
        "@types/react-dom": "^19.0.3",
        "rollup": "^4.30.1",
        "rollup-plugin-dts": "^6.1.1",
        "rollup-plugin-peer-deps-external": "^2.2.4",
        "tslib": "^2.8.1"
    }
}